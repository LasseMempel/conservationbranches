    skos:inScheme <https://www.w3id.org/archlink/branches/B51DAF> ;
    skos:notation "B8B69D" ;
    skos:note "Hermann Kühn, Conservation and Restoration of Works of Art and Antiquities. Volume 1 (London 1986), S. 155/156."@de ;
    skos:prefLabel "Bernstein"@de,
        "amber"@en .

