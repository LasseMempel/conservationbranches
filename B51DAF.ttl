    skos:altLabel "Alpaka"@de ;
    skos:broader <https://www.w3id.org/archlink/terms/conservationthesaurus/F8DFCG> ;
    skos:definition "tierische Fasern aus dem Wollhaar des Alpakas"@de ;
    skos:inScheme <https://www.w3id.org/archlink/branches/B51DAF> ;
